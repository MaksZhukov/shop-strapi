{
    "kind": "collectionType",
    "collectionName": "products",
    "info": {
        "singularName": "product",
        "pluralName": "products",
        "displayName": "Product",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "description": {
            "type": "richtext"
        },
        "images": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        },
        "category": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::category.category"
        },
        "price": {
            "type": "integer"
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": false
        }
    }
}
