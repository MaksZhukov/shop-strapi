{
  "kind": "singleType",
  "collectionName": "page_products",
  "info": {
    "singularName": "page-product",
    "pluralName": "page-products",
    "displayName": "Page product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "linksWithImages": {
      "type": "component",
      "repeatable": true,
      "component": "general.link-and-image"
    },
    "textAfterDescription": {
      "type": "richtext",
      "default": "Товар снят с б/у {autoSynonyms}, проверен. На данный момент деталь отвечает техническим и эстетическим требованиям. Если хотите получить более детальную информацию - свяжитесь с нашим менеджером."
    },
    "autoSynonyms": {
      "type": "string",
      "default": "авто"
    },
    "benefits": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "defaultSparePartSeo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.short-seo"
    },
    "defaultWheelSeo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.short-seo"
    },
    "defaultCabinSeo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.short-seo"
    },
    "defaultTireSeo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.short-seo"
    },
    "textAfterBenefits": {
      "type": "richtext"
    }
  }
}
