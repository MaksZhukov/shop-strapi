{
  "kind": "collectionType",
  "collectionName": "spare_parts",
  "info": {
    "singularName": "spare-part",
    "pluralName": "spare-parts",
    "displayName": "Spare part",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "default": "Оригинальная запчасть, которая прошла тщательную проверку перед продажей. Она подходит для данной модели авто. Товар находится в хорошем состоянии и готов к установке. В хорошем состоянии. Из Европы. Доставка"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "decimal",
      "default": 0
    },
    "priceUSD": {
      "type": "decimal"
    },
    "slug": {
      "type": "uid",
      "targetField": "h1"
    },
    "year": {
      "type": "integer"
    },
    "bodyStyle": {
      "type": "string"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "акпп",
        "мкпп",
        "робот",
        "вариатор"
      ]
    },
    "model": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::model.model",
      "inversedBy": "spareParts"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "spareParts"
    },
    "kindSparePart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::kind-spare-part.kind-spare-part",
      "inversedBy": "spareParts"
    },
    "generation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::generation.generation",
      "inversedBy": "spareParts"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "fuel": {
      "type": "enumeration",
      "enum": [
        "бензин",
        "дизель",
        "электро",
        "гибрид"
      ]
    },
    "snippets": {
      "type": "component",
      "repeatable": false,
      "component": "product.snippets"
    },
    "h1": {
      "type": "string"
    },
    "discountPrice": {
      "type": "decimal"
    },
    "discountPriceUSD": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "sparePart"
      ],
      "default": "sparePart",
      "required": true
    },
    "engine": {
      "type": "string"
    },
    "volume": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::engine-volume.engine-volume",
      "inversedBy": "spareParts"
    },
    "engineNumber": {
      "type": "string"
    },
    "sold": {
      "type": "boolean",
      "default": false
    },
    "videoLink": {
      "type": "string"
    },
    "priceRUB": {
      "type": "integer"
    }
  }
}
