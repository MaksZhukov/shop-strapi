{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "cars"
    },
    "model": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::model.model",
      "inversedBy": "cars"
    },
    "manufactureDate": {
      "type": "date"
    },
    "deliveryDate": {
      "type": "date"
    },
    "fuel": {
      "type": "enumeration",
      "enum": [
        "бензин",
        "дизель",
        "электро",
        "гибрид"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "engine": {
      "type": "string"
    },
    "bodyStyle": {
      "type": "string"
    },
    "mileage": {
      "type": "integer"
    },
    "generation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::generation.generation",
      "inversedBy": "cars"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "videoLink": {
      "type": "string"
    },
    "volume": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::engine-volume.engine-volume",
      "inversedBy": "cars"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "акпп",
        "мкпп",
        "робот",
        "вариатор"
      ]
    },
    "name": {
      "type": "string"
    },
    "code": {
      "type": "biginteger",
      "unique": true
    }
  }
}
